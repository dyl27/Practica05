public class Ciclofor{
  public static void main(String[] args){

boolean primo = false;  
    for(int i = 0; i < 1000; i++){
     primo = true;
      for(int j = 2; j < i/2 ; j++){
           if(i % j == 0){
              primo = false;
              break;
           }
      }
     if(primo){
       System.out.println("Es primo el numero: " + i );
     }
    } 


  }
}   
  