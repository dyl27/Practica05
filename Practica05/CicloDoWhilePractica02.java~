public class CicloDoWhile{
	public static void main(String[] args){
      int i = 2;
      int j = 2;
      boolean esPrimo;
      do{
       esPrimo = true; 
       j = 2;  
       while(j <= i/2){
         if(i % j == 0){
           esPrimo = false;
           break;
         }
        j++;
       }   
       if (esPrimo) {
       	System.out.println("Es numero Primo : " + i);
       }
       i++;
      }while(i <= 1000);





	}
}